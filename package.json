{
  "name": "parking-management-be",
  "version": "1.0.0",
  "description": "Backend for Parking Management System using Express, Prisma, Socket.IO and PostgreSQL (Neon)",
  "main": "src/server.js",
  "type": "module",
  "scripts": {
    "dev": "nodemon src/server.js",
    "start": "node src/server.js",
    "db:push": "npx prisma db push",
    "db:deploy": "npx prisma migrate deploy",
    "deploy": "npm run db:deploy && npm start",
    "prisma": "prisma"
  },
  "dependencies": {
    "express": "^4.21.1",
    "cors": "^2.8.5",
    "socket.io": "^4.8.1",
    "@prisma/client": "^6.17.1",
    "dotenv": "^17.2.3"
  },
  "devDependencies": {
    "nodemon": "^3.1.10",
    "prisma": "^6.17.1"
  },
